{
  "name": "json-placeholder-simple-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt && yarn run-json-server",
    "run-json-server": "json-server --watch db.json -p 3001",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lintfix": "yarn lint:js --fix",
    "serve": "concurrently \"yarn:run-json-server\" \"yarn:dev\""
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "@vuesimple/vs-pagination": "^1.4.3",
    "core-js": "3.19.3",
    "json-server": "0.17.0",
    "lodash": "4.17.21",
    "nuxt": "2.15.8",
    "vue": "2.6.14",
    "vue-lodash": "2.1.2",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "vue-textarea-autosize": "^1.1.1",
    "webpack": "4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@nuxtjs/eslint-config": "8.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/style-resources": "1.2.1",
    "@nuxtjs/svg": "0.4.0",
    "concurrently": "7.1.0",
    "eslint": "8.4.1",
    "eslint-plugin-nuxt": "3.1.0",
    "eslint-plugin-vue": "8.2.0",
    "node-sass": "7.0.1",
    "sass": "1.50.0",
    "sass-loader": "10"
  }
}
